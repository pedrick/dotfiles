Config  { font = "-*-Fixed-Bold-R-Normal-*-13-*-*-*-*-*-*-*"
        , bgColor = "black"
        , fgColor = "grey"
        , position = Top L 90
        , commands = [ Run Battery ["-t","Batt: <left>% / <timeleft>"] 10
                     , Run Date "%a %b %_d %l:%M" "date" 10
                     , Run Wireless "wlan0" [] 10
                     , Run StdinReader
                     , Run Com "/home/pedrick/util/volume.sh" ["get"] "vol" 1
                     ]
        , sepChar = "%"
        , alignSep = "}{"
        , template = "%StdinReader% }{ %battery% | Vol: %vol% | %wlan0wi% | %date% "
        , lowerOnStart = False
        }